{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "C++ Debug (MSYS2 MinGW64)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/main.exe",
      "cwd": "${workspaceFolder}",
      "MIMode": "gdb",
      "miDebuggerPath": "C:/msys64/mingw64/bin/gdb.exe",
      "console": "integratedTerminal",
      "stopAtEntry": false,
      "preLaunchTask": "Build main.cpp"
    }
  ]
}
